{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "vs-mobile-center-docs/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "vs-mobile-center-docs/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "contributors_to_exclude": [
        "conceptdev",
        "meganbradley",
        "timeyoutakeit",
        "jwargo"
      ],
      "breadcrumb_path": "~/_breadcrumb/toc.yml",
      "titleSuffix": "Visual Studio App Center",
      "extendBreadcrumb": true,
      "manager": "lucen",
      "audience": "developer",
      "ms.service": "vs-appcenter",
      "ms.topic": "article",
      "searchScope": [
        "App Center"
      ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.vs-mobile-center-docs"
        }
      },
      "feedback_system": "Standard",
      "feedback_product_url": "https://docs.microsoft.com/appcenter/help#accessing-app-center-free-support",
      "uhfHeaderId": "MSDocsHeader-AppCenter"
    },
    "fileMetadata": {
      "ms.service": {
        "analytics/**/**.md":"vs-appcenter",
        "api-docs/**/**.md":"vs-appcenter",
        "auth/**/**.md":"vs-appcenter",
        "build/**/**.md":"vs-appcenter",
        "cli/**/**.md":"vs-appcenter",
        "dashboard/**/**.md":"vs-appcenter",
        "data/**/**.md":"vs-appcenter",
        "diagnostics/**/**.md":"vs-appcenter",
        "distribution/**/**.md": "vs-appcenter",
        "errors/**/**.md":"vs-appcenter",
        "gdpr/**/**.md":"vs-appcenter",
        "general/**/**.md":"vs-appcenter",
        "migration/**/**.md":"vs-appcenter",
        "test-cloud/**/**.md":"vs-appcenter",
        "transition/**/**.md": "vs-appcenter",
        "sdk/**/**.md":"vs-appcenter-sdk"
      },
      "ms.subservice": {
        "analytics/**/**.md":"app-center-analytics",
        "api-docs/**/**.md":"app-center-general",
        "auth/**/**.md":"app-center-auth",
        "build/**/**.md":"app-center-build",
        "cli/**/**.md":"app-center-general",
        "dashboard/**/**.md":"app-center-general",
        "data/**/**.md":"app-center-data",
        "diagnostics/**/**.md":"app-center-diagnostics",
        "distribution/**/**.md": "app-center-distribute",
        "errors/**/**.md":"app-center-general",
        "gdpr/**/**.md":"app-center-general",
        "general/**/**.md":"app-center-general",
        "migration/**/**.md":"app-center-general",
        "test-cloud/**/**.md":"app-center-test",
        "transition/**/**.md": "app-center-transition",
        "sdk/analytics/**/**.md":"app-center-analytics",
        "sdk/auth/**/**.md":"app-center-auth",
        "sdk/data/**/**.md":"app-center-data",
        "sdk/diagnostics/**/**.md":"app-center-diagnostics",
        "sdk/distribute/**/**.md": "app-center-distribute",
        "sdk/getting-started/**/**.md":"app-center-general",
        "sdk/other-apis/**/**.md":"app-center-general",
        "sdk/troubleshooting/**/**.md":"app-center-general"
      }
    },
    "template": [],
    "dest": "vs-mobile-center-docs",
    "markdownEngineName": "markdig",
    "rules": {
      "sensitive-language-wl": {
        "exclude": [
          "distribution/codepush/cordova.md"
        ]
      }
    }
  }
}
